{
  "name": "cee-render",
  "version": "0.0.5",
  "description": "Librería renderizado del CEE",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:verbose": "jest --verbose",
    "test:watch": "jest --watchAll",
    "build": "npm run test && rimraf ./build && gulp",
    "prepare": "npm run build",
    "publish:server": "npm publish"
  },
  "keywords": [
    "TS",
    "Javascript",
    "npm"
  ],
  "author": "Alejandro Frías",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alejandrofdiaz/cee-render.git"
  },
  "bugs": {
    "url": "https://github.com/alejandrofdiaz/cee-render/issues"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.2",
    "@types/lodash": "^4.14.106",
    "@types/node": "^9.6.2",
    "@types/node-sass": "^3.10.32",
    "@types/puppeteer": "^1.2.1",
    "@types/xml-js": "^1.0.0",
    "babel-core": "^6.26.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-remove-console": "^6.9.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.4.0",
    "express": "^4.16.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.2",
    "jest": "^22.4.3",
    "lodash": "^4.17.5",
    "merge2": "^1.2.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.2",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "node-sass": "^4.8.3",
    "puppeteer": "^1.2.0",
    "xml-js": "^1.6.2"
  }
}
